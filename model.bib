
@phdthesis{__2016,
	address = {青岛},
	type = {硕士},
	title = {中国近海平均海平面的确定及黑潮提取研究},
	abstract = {卫星测高能够在全球范围内,全天候重复获取高精度的海面高观测值,为海洋科学研究提供了海量的数据。从早期的Skylab试验卫星到新一代测高卫星,卫星测高技术不断发展完善,数据处理手段日趋成熟,其空间分辨率不断提高,目前被广泛应用于海洋学、地球物理学、大地测量学等学科的研究。平均海平面是研究海洋动力环境科学的重要基础,是目前卫星大地测量学研究的重要问题之一,本文采用我国自主研制的HY-2A卫星数据,联合T/P、Jason-1、Jason-2、Envisat、Cryosat-2等多源测高卫星数据。通过数据编辑与筛选,剔除陆地、岛屿、海冰、礁石、近岸等无效测高数据,选择质量较好的测高数据;同时在中国近岸区域,使用Jason-1卫星SGDR数据,利用多前缘多阈值算法计算中国近岸区域海面高用以改善近岸数据精度;采用FES2004海洋潮汐模型进行海洋潮汐校正,经过共线处理、交叉点平差、数据基准统一、数据网格化等方法,消除海面时变效应、径向轨道误差和卫星系统性偏差等影响,并确定中国近海及其附近海域(0°N-40°N,102°E-138°E)2′×2′平均海平面模型,与MSS\_CNES\_2010和MSS\_CNES\_2011模型从定性和定量两个方面进行分析对比,RMS值分别为11.2cm和13.2cm,验证了本文所建立的模型是可靠的。基于本文所建立的平均海平面模型,在上述基础上开展中国近海黑潮提取研究,结合EGM2008重力场模型,计算稳态海面地形(MDT),与MDT\_CNES\_CLS2013稳态海面地形模型相比,RMS值为15.6cm。通过地转流方程计算地转流东分量和北分量,提取中国近海海域黑潮信息,结果能够较好反映黑潮流经区域、流速和弯曲现象,为HY-2高度计数据产品制作提供服务,拓展HY-2A高度计数据的应用领域。},
	school = {中国石油大学(华东)},
	author = {管一鹤},
	year = {2016},
	keywords = {卫星测高;交叉点平差;基准统一;平均海平面;黑潮}
}

@article{__2012,
	title = {基于多颗在轨高度计数据的中国近海平均海平面模型建立},
	volume = {34},
	abstract = {联合使用Jason-2成功发射3a来的Jason-2(cycle 001-113)、轨道调整前后的Envisat(cycle070-107)以及轨道调整后的Jason-1(cycle 263-352)等在轨高度计卫星数据,首先对电离层延迟校正项进行平滑处理,再经过数据编辑和各项地球物理及环境改正后,对周期(cycle)逐一进行统计验证并剔除交叉点不符值异常的数据周期,采用共线处理和交叉点平差削弱海面时变和径向轨道误差的影响,再经参考椭球和参考框架基准的统一,最后选用Shepard方法建立了我国海域及邻海海域(0°～45°N,100°～140°E)2′×2′分辨率的平均海平面模型。将所建立模型与MSS-CNES-CLS01模型和MSS-CNES-CLS10模型进行了比较,不符值RMS分别为8.28和11.65cm,验证了所开展模型的正确性。},
	number = {06},
	journal = {海洋学报(中文版)},
	author = {张胜军 and 万剑华 and 杨俊钢},
	year = {2012},
	note = {ISBN: 0253-4193},
	keywords = {卫星测高;电离层平滑;平差;网格化;平均海平面},
	pages = {66--73}
}

